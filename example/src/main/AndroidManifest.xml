<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.socratica.mobile.example" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.socratica.mobile.example.ExampleListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.socratica.mobile.example.BigImageActivity"
            android:label="@string/title_activity_big_image"
            android:parentActivityName="com.socratica.mobile.example.ExampleListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ExampleListActivity" />
        </activity>
    </application>

</manifest>
